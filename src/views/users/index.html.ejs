<h2>Users</h2>
<% if (!usersList.length) { %>
  <p>No results</p>
<% } else { %>
  <table id="users-list">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Rating</th>
        <th>CV</th>
        <th>Image</th>
        <th>Occupation</th>
        
      </tr>
    </thead>
    <tbody>
      <% usersList.forEach((user) => { %>
        <tr>
          <td><a href="<%= showUserPath(user) %> "><%= user.name %></a></td>
          <td><%= user.email %></td>
          <td><%= user.rating %></td>
          <td><%= user.cvPath %></td>
          <td><%= user.imagePath %></td>
          <td><%= user.occupation %></td>
        </tr>
      <% }) %>
    </tbody>
  </table>
<% } %>

<% if (currentUser) { %>
  <a href="<%= newUserPath %>">New User</a>
<% } %>