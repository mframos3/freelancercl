  <table id="users-list">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Rating</th>
        <th>CV</th>
        <th>Image</th>
        <th>Occupation</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.rating %></td>
          <td><%= user.cvPath %></td>
          <td><%= user.imagePath %></td>
          <td><%= user.occupation %></td>
        </tr>
    </tbody>
  </table>