  <table id="users-list">
    <thead>
      <tr>
        <th>Name</th>
        <th>Email</th>
        <th>Rating</th>
        <th>CV</th>
        <th>Image</th>
        <th>Occupation</th>
      </tr>
    </thead>
    <tbody>
        <tr>
          <td><%= user.name %></td>
          <td><%= user.email %></td>
          <td><%= user.rating %></td>
          <td><%= user.cvPath %></td>
          <td><%= user.imagePath %></td>
          <td><%= user.occupation %></td>
        </tr>
    </tbody>
  </table>

  <% if (currentUser) { %>
      <form action="<%= uploadCvPath %>" enctype="multipart/form-data" method="post"> Upload CV
        <div class="field">
          <label for="list">Select file</label>
          <input type="file" name="uploadCv" />
        </div>
        <div class="field">
          <input type="submit" name="uploadCv" value="Upload" />
        </div>
      </form>
  <% } %>