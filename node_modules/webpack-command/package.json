{
  "name": "webpack-command",
  "version": "0.5.0",
  "description": "A superior CLI experience for webpack.",
  "license": "MIT",
  "repository": "shellscape/webpack-command",
  "author": "Andrew Powell <andrew@shellscape.org>",
  "homepage": "https://github.com/shellscape/webpack-command",
  "bugs": "https://github.com/shellscape/webpack-command/issues",
  "bin": {
    "webpack": "lib/cli.js",
    "webpack-command": "lib/cli.js",
    "wp": "lib/cli.js"
  },
  "main": "lib/index.js",
  "engines": {
    "node": ">= 8.9.0"
  },
  "scripts": {
    "commitlint": "commitlint",
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint --cache lib test",
    "ci:lint:commits": "commitlint --from=${CIRCLE_BRANCH} --to=${CIRCLE_SHA1}",
    "lint-staged": "lint-staged",
    "prepublishOnly": "mkdir -p data && cp -r lib/commands/defaults.json data/commands.json",
    "security": "npm audit",
    "test": "npm run prepublishOnly && mocha test/test.js --exit --timeout=5000",
    "test:coverage": "mkdir -p coverage && nyc --silent npm test && npm run test:coverage:report",
    "test:coverage:report": "nyc report --reporter=lcov --reporter=text-lcov --reporter=json --reporter=clover > coverage/lcov.info",
    "ci:lint": "npm run lint && npm run security",
    "ci:test": "npm run test -- --runInBand",
    "ci:coverage": "npm run test:coverage -- --runInBand"
  },
  "files": [
    "data/",
    "lib/"
  ],
  "peerDependencies": {
    "webpack": "^4.4.0"
  },
  "dependencies": {
    "@webpack-contrib/config-loader": "^1.2.0",
    "@webpack-contrib/schema-utils": "^1.0.0-beta.0",
    "camelcase": "^5.0.0",
    "chalk": "^2.3.2",
    "debug": "^4.1.1",
    "decamelize": "^3.2.0",
    "enhanced-resolve": "^4.0.0",
    "import-local": "^3.0.2",
    "isobject": "^4.0.0",
    "loader-utils": "^1.1.0",
    "log-symbols": "^3.0.0",
    "loud-rejection": "^2.1.0",
    "meant": "^1.0.1",
    "meow": "^5.0.0",
    "merge-options": "^1.0.0",
    "object.values": "^1.0.4",
    "opn": "^6.0.0",
    "ora": "^3.4.0",
    "plur": "^3.0.0",
    "pretty-bytes": "^5.0.0",
    "strip-ansi": "^5.2.0",
    "text-table": "^0.2.0",
    "titleize": "^2.1.0",
    "update-notifier": "^3.0.1",
    "v8-compile-cache": "^2.0.0",
    "webpack-log": "^3.0.1",
    "wordwrap": "^1.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.1.0",
    "@commitlint/config-conventional": "^8.1.0",
    "babel-jest": "^24.9.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.1",
    "camelcase-keys": "^6.0.1",
    "codecov": "^3.0.0",
    "crc": "^3.5.0",
    "cross-env": "^5.1.4",
    "decamelize-keys": "^1.1.0",
    "del": "^5.0.0",
    "del-cli": "^2.0.0",
    "eslint": "^6.2.1",
    "eslint-config-shellscape": "^2.0.2",
    "execa": "^0.10.0",
    "expect": "^24.9.0",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^3.0.4",
    "jest-serializer-path": "^0.1.14",
    "jest-snapshot": "^23.0.1",
    "lint-staged": "^9.2.3",
    "memory-fs": "^0.4.1",
    "minimist": "^1.2.0",
    "minimist-options": "^4.0.1",
    "mocha": "^6.2.0",
    "nyc": "^14.1.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.11.1",
    "standard-version": "^7.0.0",
    "stylelint-webpack-plugin": "^0.10.4",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "webpack": "^4.23.1"
  },
  "keywords": [
    "webpack"
  ],
  "pre-commit": "lint-staged",
  "lint-staged": {
    "*.js": [
      "eslint --fix",
      "git add"
    ]
  },
  "nyc": {
    "include": [
      "lib/**/*.js"
    ]
  }
}
